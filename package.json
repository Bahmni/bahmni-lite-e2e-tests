{
  "name": "gauge-taiko-template",
  "description": "Starter template for writing JavaScript tests for Gauge",
  "scripts": {
    "dev": "gauge run specs --env local -v --tags 'dev'",
    "local": "gauge run specs --env local -v -p -n=5 --tags 'devtest'",
    "qa": "gauge run specs/UI --env cure/qa -v -p -n=4 --tags 'devtest'",
    "uat": "gauge run specs/UI --env cure/uat -v -p -n=4 --tags 'cure'",
    "failed":"gauge run --failed",
    "ci": "gauge run specs -v "
  },
  "dependencies": {
    "axios": "^0.27.2",
    "buffer": "latest",
    "csvtojson": "latest",
    "html-reporter": "^9.10.1",
    "json2csv": "latest",
    "pdf-parse": "^1.1.1",
    "supertest": "^6.3.3",
    "taiko": "1.3.10",
    "uuid": "latest"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0"
  }
}
